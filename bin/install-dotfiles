#!/bin/bash -x
cd ~/dotfiles

for file in tmux.conf zsh zshrc gitignore_global vim gvimrc vimrc gitconfig gemrc irbrc ackrc railsrc vimrc.bundles agignore
do
  echo "Linking $file to ~/.$file..";
  rm ~/.$file;
  ln -sf ~/dotfiles/$file ~/.$file;
done

ln -sf /Users/Rob/Dropbox/Apps/tmuxinator ~/.tmuxinator
